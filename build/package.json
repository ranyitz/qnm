{
    "name": "qnm",
    "version": "0.13.3",
    "description": "cli utility for querying the node_modules directory",
    "bin": "bin/qnm.js",
    "repository": "ranyitz/qnm",
    "engines": {
        "node": ">=8.7"
    },
    "author": "Ran Yitzhaki",
    "license": "MIT",
    "keywords": [
        "query",
        "node_modules",
        "version",
        "cli"
    ],
    "scripts": {
        "test": "jest",
        "lint": "eslint .",
        "build": "tsc",
        "watch": "tsc --watch",
        "test:watch": "jest --watch",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "@types/node": "^12.12.8",
        "@types/update-notifier": "^2.5.0",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-yoshi-base": "^4.4.2",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jest": "^21.15.2",
        "eslint-plugin-prettier": "^2.6.0",
        "prettier": "^1.11.1",
        "husky": "^0.14.3",
        "jest": "^23.0.0",
        "lint-staged": "^7.1.2",
        "typescript": "^3.7.2"
    },
    "dependencies": {
        "archy": "^1.0.0",
        "chalk": "^2.4.1",
        "commander": "^4.0.1",
        "figures": "^2.0.0",
        "fuzzysort": "^1.1.1",
        "js-levenshtein": "^1.1.3",
        "lodash": "^4.17.5",
        "log-update": "^2.3.0",
        "opn": "^5.3.0",
        "pkg-dir": "^2.0.0",
        "prompts": "^0.1.8",
        "tabtab": "^2.2.2",
        "update-notifier": "^3.0.1"
    },
    "files": [
        "bin",
        "src",
        "CHANGELOG.md",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "lint-staged": {
        "*.js": [
            "eslint"
        ],
        "*.{md,json}": [
            "prettier"
        ]
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/*.spec.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/fixtures/"
        ],
        "watchPathIgnorePatterns": [
            "node_modules"
        ],
        "setupFiles": [
            "./__tests__/setup.js"
        ]
    }
}
